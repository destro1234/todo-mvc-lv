<section class="todoapp">
  <header class="header">
    <h1>Signup!</h1>

    <%= form_for(@user) do |f| %>
      <%= f.text_field :email, :class => "new-todo" %>
    <% end %>

    <form action="/users" method="post">
      <input type="submit">
    </form>

  </header>

  <footer class="footer">
  </footer>
</section>
